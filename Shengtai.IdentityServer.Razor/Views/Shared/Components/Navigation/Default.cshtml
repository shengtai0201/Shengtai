@model (IList<Shengtai.IdentityServer.Models.Shared.INavHeader> Headers, IList<string> Roles)
@foreach (var header in Model.Headers)
{
    if (header.Show(Model.Roles))
    {
        if (!string.IsNullOrEmpty(header.Text))
        {
            <li class="nav-header">@header.Text</li>
        }
        foreach (var nav in header.Menus)
        {
            if (nav.Show(Model.Roles))
            {
                var itemClass = nav.Active ? "menu-open" : "";
                <li class='nav-item @itemClass'>
                    @await Html.PartialAsync("~/Views/Shared/AdminLTE/_Menu.cshtml", nav)
                </li>
            }
        }
    }
}